<div class="container">
  <% @benefits.each do |benefit| %>
    <div class="form-columns d-flex flex-row justify-content-center">
      <div class="right-column col-12 col-md-6">
          <%= benefit.name %>
          <%= benefit.quantity %>
          <%= benefit.value_in_points %>
      </div>
    </div>
  <% end %>
</div>
